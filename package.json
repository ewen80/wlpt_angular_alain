{
  "name": "waap",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng s -o",
    "build": "npm run ng-high-memory build -- --configuration production",
    "build-test": "npm run ng-high-memory build -- --configuration test",
    "test": "ng test",
    "lint": "npm run lint:ts",
    "e2e": "ng e2e",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "hmr": "ng s -o --hmr",
    "analyze": "npm run ng-high-memory build -- --configuration production --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.14",
    "@angular/common": "^12.2.14",
    "@angular/compiler": "^12.2.14",
    "@angular/core": "^12.2.14",
    "@angular/forms": "^12.2.14",
    "@angular/platform-browser": "^12.2.14",
    "@angular/platform-browser-dynamic": "^12.2.14",
    "@angular/router": "^12.2.14",
    "@delon/abc": "^12.4.2",
    "@delon/acl": "^12.4.2",
    "@delon/auth": "^12.4.2",
    "@delon/cache": "^12.4.2",
    "@delon/chart": "^12.4.2",
    "@delon/form": "^12.4.2",
    "@delon/mock": "^12.4.2",
    "@delon/theme": "^12.4.2",
    "@delon/util": "^12.4.2",
    "ajv": "^8.8.2",
    "ajv-formats": "^2.1.1",
    "ngx-tinymce": "^12.0.0",
    "ngx-ueditor": "^12.0.0",
    "postcss": "^8.4.4",
    "rxjs": "^6.6.0",
    "screenfull": "^5.2.0",
    "ts-md5": "^1.2.10",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.14",
    "@angular-eslint/builder": "~12.3.1",
    "@angular-eslint/eslint-plugin": "~12.3.1",
    "@angular-eslint/eslint-plugin-template": "~12.3.1",
    "@angular-eslint/schematics": "~12.3.1",
    "@angular-eslint/template-parser": "~12.3.1",
    "@angular/cli": "^12.2.14",
    "@angular/compiler-cli": "^12.2.14",
    "@angular/language-service": "^12.2.14",
    "@delon/testing": "^12.4.2",
    "@types/file-saver": "^2.0.4",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.37",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^2.2.1",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-jsdoc": "^36.0.8",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-prettier": "^2.2.1",
    "eslint-plugin-react": "^7.27.1",
    "file-saver": "^2.0.4",
    "husky": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-alain": "^12.4.2",
    "ng-alain-plugin-theme": "^12.0.0",
    "ng-zorro-antd": "^12.1.0",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "(src)/**/*.{html,ts}": [
      "eslint --fix"
    ],
    "(src)/**/*.less": [
      "stylelint --syntax less --fix"
    ]
  }
}
